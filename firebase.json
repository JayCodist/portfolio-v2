{
  "hosting": {
    "site": "johnson-ubaezuonu",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "europe-west1"
    },
    headers: {
      "regex": ".+/\w+\.(jpg|jpeg|png|svg|js|css)$",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=31536000"
      } ]
    }
  }
}
